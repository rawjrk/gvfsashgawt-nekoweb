<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('/partials/head', { title }) %>
    <link rel="stylesheet" href="/styles/global.css" />
    <%- include('/partials/metadata', { metadata }) %>
  </head>

  <body>
    <%- include('/partials/pageH1') %>

    <main>
      <p>
        Oops! Sorry, but the page you have requested contains complete
        <!-- navigation links -->
        <% navigation.forEach((link) => { %>
        <span class="gib-fill"></span>
        <a class="gib-link" href="<%= link.href %>"><%= link.text %></a>
        <% }) %>
        <span class="gib-fill"></span>.
      </p>
    </main>

    <script src="/scripts/lib/gib-core.js"></script>
    <script src="/scripts/lib/gib-events.js"></script>
    <script src="/scripts/lib/datamosh.js"></script>
    <script src="/scripts/pages/home.js"></script>
  </body>
</html>
